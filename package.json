{
  "name": "weather-ural",
  "version": "1.0.0",
  "engines": {
    "node": "16.x"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "description": "App-CLI",
  "main": "main.js",
  "scripts": {
    "build": "tsc -p .",
    "create": "npm run build && npm run test",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/main.ts",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TimurKurbanov1994/weather-ural"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.1.3",
    "commander": "^9.4.1",
    "geolib": "^3.3.3",
    "ip": "^1.1.8"
  },
  "devDependencies": {
    "@types/node": "^18.11.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "semantic-release": "^19.0.5"
  },
  "files": [
    "dist"
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    "@semantic-release/github"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./src/main.ts",
        "default": "./dist/main.js"
      },
      "require": {
        "types": "./src/main.ts",
        "default": "./dist/main.js"
      }
    }
  }
}
